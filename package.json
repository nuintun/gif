{
  "name": "@nuintun/gif",
  "version": "0.0.1",
  "description": "A faster static gif encoding library use WebAssembly.",
  "main": "index.js",
  "typings": "typings/index.d.ts",
  "scripts": {
    "test": "node tests",
    "asbuild": "npm run asbuild:untouched && npm run asbuild:optimized",
    "asbuild:untouched": "asc assembly/index.ts --target debug --exportRuntime",
    "asbuild:optimized": "asc assembly/index.ts --target release --exportRuntime"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuintun/gif.git"
  },
  "keywords": [
    "gif",
    "wasm"
  ],
  "author": {
    "name": "nuintun",
    "email": "nuintun@qq.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nuintun/gif/issues"
  },
  "homepage": "https://github.com/nuintun/gif#readme",
  "dependencies": {
    "@assemblyscript/loader": "^0.18.0"
  },
  "devDependencies": {
    "rimraf": "^3.0.2",
    "rollup": "^2.38.1",
    "typescript": "^4.1.3",
    "magic-string": "^0.25.7",
    "assemblyscript": "^0.18.0",
    "@rollup/plugin-wasm": "^5.1.2",
    "@rollup/plugin-typescript": "^8.1.0",
    "@rollup/plugin-node-resolve": "^11.1.0"
  }
}